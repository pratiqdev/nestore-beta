{
  "name": "nestore",
  "version": "1.4.3",
  "description": "",
  "main": "index.js",
  "type": "module",
  "types": "dist/nestore.d.ts",
  "files": [
    "./index.js",
    "./README.md",
    "dist/"
  ],
  "scripts": {
    "lint": "clear && eslint --fix -c .eslintrc.json .",
    "build-usenestore": "cd ../utils/useNestore && yarn build",
    "build-persistAdapter": "cd ../utils/persistAdapter&& yarn build",
    "build-mongoAdapter": "cd ../utils/mongoAdapter && yarn build",
    "build": "clear && rm -rf ./dist && tsc",
    "build-umd": "clear && rm -rf ./dist_umd && tsc -p tsconfig.umd.json",
    "test": "clear && mocha --config .mocharc.cjs",
    "watch-test": "clear && tsc && nodemon",
    "test-nobuild": "clear && mocha --config .mocharc.cjs",
    "debug": "clear && tsc && DEBUG=nestore:** mocha --config .mocharc.cjs",
    "runnit": "clear && rm -rf ./dist && tsc && DEBUG=nestore:** node index.js",
    "runnit-quiet": "clear && tsc && node index.js",
    "runnit-windows": "clear && tsc && set DEBUG=nestore:** && node index.js",
    "roll": "rollup --config rollup.config.js",
    "bump": "npm version patch --force",
    "pub": "npm publish --access public"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "eventemitter2": "^6.4.7",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "debug": "^4.3.4",
    "@babel/eslint-parser": "^7.19.1",
    "@open-wc/building-rollup": "^2.0.2",
    "@types/debug": "^4.1.7",
    "@types/lodash-es": "^4.17.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "17.0.29",
    "@types/react": "^18.0.21",
    "@types/spellchecker": "^3.5.0",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.40.1",
    "chai": "^4.3.6",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.18",
    "typescript": "*"
  },
  "peerDependencies": {
    "mongodb": "^4.10.0",
    "react": "^18.2.0"
  }
}
